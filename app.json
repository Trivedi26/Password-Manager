{
  "expo": {
    "name": "Pin Keeper",
    "slug": "pinkeeper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pinkeeper.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.Pinkeeper.pinkeeper",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pinkeeper.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/pinkeeper.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-font",
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "75c4f933-56ec-4252-8b77-eacdbe77f3f3"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/cae29f45-1d44-486b-ac00-16348eac15dc"
    },
    "owner": "shubhamtrivedi26"
  },
  "cli": {
    "appVersionSource": "remote"
  }
}
